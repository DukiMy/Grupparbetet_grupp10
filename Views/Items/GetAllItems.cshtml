@{
    ViewData["Title"] = "All items";
}
@model IEnumerable<Item>

<div class="container">
    <h3 class="display-4">All items</h3>
    <div class="row">

        @foreach (var item in Model)
        {
            <div class="col-md-8">
                <partial name="_itemThumbnail" model="@item" />
            </div>
        }

    </div>
</div>
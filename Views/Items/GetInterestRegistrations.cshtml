@using HomeFinder.ViewModels

@model InterestRegistrationsViewModel

<div class="container">
    <h4 class="display-4">Alla intresseanmälningar</h4>
    <div class="row justify-content-center">
        <div class="col-6">
            <table class="table-hover">
                <th>E-postadress</th>
                @foreach (var EmailAdress in Model.UserEmails)
                {
                    <tr>
                        <td>
                            @EmailAdress
                        </td>
                    </tr>
                }
            </table>
        </div>
    </div>
</div>
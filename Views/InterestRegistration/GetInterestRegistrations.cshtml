@using HomeFinder.ViewModels

@model InterestRegistrationsListViewModel

<div class="container">
    <h4 class="display-4">Alla intresseanmälningar</h4>
    <div class="row justify-content-center">
        <div class="col-6">
            <table class="table-hover">
                <tr>
                    <th>E-postadress</th>
                </tr>
                @foreach (var interestRegistration in Model.InterestRegistrations)
                {
                    <tr>
                        <td>
                            @interestRegistration.UserEmail
                        </td>
                    </tr>
                }
            </table>
        </div>
    </div>
</div>
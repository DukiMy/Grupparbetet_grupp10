@using HomeFinder.ViewModels

@model InterestRegistrationsListViewModel

    <div class="container card box-shadow">
        <div class="row p-2 justify-content-center">
            <h5 class="display-5 col-md-12">Alla intresseanmälningar</h5>
            <div class="col-md-12">
                <table class="table-hover">
                    <tr>
                        <th>E-postadress</th>
                    </tr>
                    @foreach (var interestRegistration in Model.InterestRegistrations)
                    {
                        <tr>
                            <td>
                                @interestRegistration.UserEmail
                            </td>
                        </tr>
                    }
                </table>
            </div>
        </div>
    </div>
